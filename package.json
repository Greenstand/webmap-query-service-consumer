{
  "name": "treetracker",
  "version": "1.2.0",
  "private": true,
  "engines": {
    "node": ">=16",
    "npm": ">=6.0.0"
  },
  "main": "server/server.js",
  "scripts": {
    "start": "tsc && node dist/server.js",
    "dev": "nodemon --watch 'server/**/*.ts' --exec 'ts-node' server/server.ts",
    "build": "tsc",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier ./ --write",
    "format": "npm run prettier; npm run lint:fix",
    "server": "nodemon server/server.js",
    "server-test": "DEBUG=express:* NODE_LOG_LEVEL=debug nodemon server/serverTest.js",
    "test": "npm run test-unit; npm run test-integration;npm run test-repository",
    "test-unit": "NODE_ENV=test mocha --exit --ignore './server/infra/database/*.spec.js' ./server/**/*.spec.js",
    "test-repository": "NODE_ENV=test mocha -r dotenv/config dotenv_config_path=.env.test --exit ./server/infra/database/*.spec.js",
    "test-seedDB": "NODE_ENV=test mocha -r dotenv/config dotenv_config_path=.env.test --timeout 10000 './**/*.spec.js'",
    "test-integration": "NODE_ENV=test mocha --exit --timeout 20000 './__tests__/integration/**/*.spec.js'",
    "test-watch": "NODE_ENV=test NODE_LOG_LEVEL=info mocha -w './server/**/{,spec}.js' --recursive ./server/**/*.spec.js",
    "test-watch-debug": "NODE_ENV=test NODE_LOG_LEVEL=debug mocha  -w './server/**/{,spec}.js' ./server/**/*.spec.js",
    "encrypt": "nodecipher encrypt .env env.secret",
    "decrypt": "nodecipher decrypt env.secret .env",
    "db-migrate": "db-migrate up -v -m database/migrations/",
    "prepare": "is-ci || husky install"
  },
  "keywords": [
    "ecology"
  ],
  "author": "Greenstand Engineers",
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "@sentry/node": "^5.1.0",
    "body-parser": "^1.18.2",
    "dotenv": "^10.0.0",
    "expect-runtime": "^0.7.0",
    "express": "^4.16.2",
    "express-async-handler": "^1.1.4",
    "express-validator": "^6.4.0",
    "knex": "^0.95.12",
    "loglevel": "^1.6.8",
    "pg": "^8.7.1",
    "rascal": "^13.0.0",
    "uuid": "^8.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/chai": "^4.2.22",
    "@types/chai-arrays": "^2.0.0",
    "@types/express": "^4.17.13",
    "@types/mocha": "^9.0.0",
    "@types/mock-knex": "^0.4.3",
    "@types/node": "^16.11.6",
    "@types/rascal": "^10.0.4",
    "@types/sinon": "^10.0.6",
    "@types/sinon-chai": "^3.2.5",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "amqplib": "^0.8.0",
    "chai": "3.5.0",
    "chai-arrays": "^2.2.0",
    "chai-http": "^4.3.0",
    "database-cleaner": "^1.3.0",
    "db-migrate": "^0.11.12",
    "db-migrate-pg": "^1.2.2",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-typescript": "^14.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^7.0.4",
    "is-ci": "^3.0.1",
    "lint-staged": "^11.2.6",
    "mocha": "^8.3.2",
    "mock-knex": "^0.4.10",
    "node-cipher": "^6.3.3",
    "nodemon": "^2.0.14",
    "prettier": "^2.4.1",
    "sinon": "^9.2.4",
    "sinon-chai": "^2.14.0",
    "supertest": "^4.0.2",
    "typescript": "^4.4.4"
  },
  "directories": {
    "doc": "docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Greenstand/webmap-query-service-consumer.git"
  },
  "bugs": {
    "url": "https://github.com/Greenstand/webmap-query-service-consumer/issues"
  },
  "mocha": {
    "env": "dotenv_config_path=.env.test",
    "require": "dotenv/config"
  },
  "homepage": "https://github.com/Greenstand/webmap-query-service-consumer#readme"
}
